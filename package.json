{
  "name": "claude-from-obsidian",
  "version": "0.2.0",
  "description": "Obsidian plugin for Claude Shell communication",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": ["obsidian", "obsidian-plugin", "claude", "ai"],
  "author": "mfranc",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "obsidian": "latest",
    "prettier": "^3.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
